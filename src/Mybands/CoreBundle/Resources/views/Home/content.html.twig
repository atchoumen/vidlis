{% include 'MyBandsCoreBundle:Default:sidebar.html.twig' %}
<div id="right_content">           
    <div class="box330 box-boreder">
        <div class="mapHome" id="concert1"></div>
        <div class="boxFooter"><h2>Sigmund Stern Grove</h2></div>
        <script>
        var map = new L.Map('concert1', {
                scrollWheelZoom: false
            }
        );
        var cloudmadeUrl = 'http://{s}.tile.cloudmade.com/BC9A493B41014CAABB98F0471D759707/997/256/{z}/{x}/{y}.png',
        cloudmadeAttribution = '',
        cloudmade = new L.TileLayer(cloudmadeUrl, {maxZoom: 18, attribution: cloudmadeAttribution});
        map.setView(new L.LatLng(45.756, 4.831), 16).addLayer(cloudmade);
        map.on('click', onMapClick);
        var popup = new L.Popup();
        function onMapClick(e) {
            var latlngStr = '(' + e.latlng.lat.toFixed(3) + ', ' + e.latlng.lng.toFixed(3) + ')';
            popup.setLatLng(e.latlng);
            popup.setContent("You clicked the map at " + latlngStr);
            map.openPopup(popup);
        }
    </script>
    </div>
    <div class="box330 box-boreder">
        <div class="boxHeader"><h2>Anti Sublime</h2></div>
        <div class="boxFooter dark">
            <div class="subTitle">Artiste</div>
            <h2 class="albumTitle">Psygnosis</h2>
            <a href="#" class="btn gray">Découvrir</a>
        </div>
    </div>
